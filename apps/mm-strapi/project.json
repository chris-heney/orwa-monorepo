{
  "name": "mm-strapi",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mm-strapi/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "prune-lockfile": {
      "dependsOn": ["build"],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/mm-strapi/package.json",
        "{workspaceRoot}/dist/apps/mm-strapi/package-lock.json"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "copy-workspace-modules": {
      "dependsOn": ["build"],
      "cache": true,
      "outputs": ["{workspaceRoot}/dist/apps/mm-strapi/workspace_modules"],
      "executor": "@nx/js:copy-workspace-modules",
      "options": {
        "buildTarget": "build"
      }
    },
    "prune": {
      "dependsOn": ["prune-lockfile", "copy-workspace-modules"],
      "executor": "nx:noop"
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "strapi develop",
        "cwd": "apps/mm-strapi"
      }
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "nx:noop"
    }
  }
}
